@CONFERENCE{Bakken2007,
  author = {Bjorn H. Bakken and Hans I. Skjelbred},
  title = {Planning of Distributed Energy Supply to Suburb},
  booktitle = {Power Engineering Society General Meeting, IEEE},
  year = {2007},
  pages = {1--8},
  address = {Tampa, FL},
  month = {June},
  abstract = {In this paper, a novel model to optimize the design of local energy
	networks with respect to operation and investment cost and environmental
	consequences is demonstrated. The chosen case study is based on a
	real case from a Norwegian municipality. Several alternatives to
	supply energy to a new suburb with detached houses are considered:
	Electrical boilers, ground source heat pumps, gas boilers and a simplified
	demonstration alternative of hydrogen fuel cell based on renewable
	energy. The alternatives are ranked with respect to operational and
	investment cost. Emissions from each alternative are also accounted
	for. Due to an integrated optimization of diurnal operation and investments,
	each design alternative can be inspected in the full-graphical user
	interface to verify the utilization of individual components during
	peak load and low load periods.},
  keywords = {Energiesystemmodellierung},
  owner = {max.mustermann},
  timestamp = {2012.12.11}
}
@article{navarro-espinosa_probabilistic_2014,
	title = {Probabilistic modeling and assessment of the impact of electric heat pumps on low voltage distribution networks},
	volume = {127},
	issn = {0306-2619},
	url = {https://www.sciencedirect.com/science/article/pii/S030626191400378X},
	doi = {https://doi.org/10.1016/j.apenergy.2014.04.026},
	abstract = {Electrification of heating by making use of the Electric Heat Pump ({EHP}) technology powered by increasing shares of electricity renewable sources is seen as a potential key approach to decarbonise the energy sector in many countries, and especially in the {UK}. However, the widespread use of {EHPs} in substitution of fuel boilers might cause significant issues in terms of electrical distribution network impact, particularly at the low voltage ({LV}) level. This has not been addressed properly in the studies carried out so far also due to lack of available data and suitable models. In this light, this paper introduces a novel and comprehensive probabilistic methodology based on Monte Carlo simulations and a relevant tool to assess the impact of {EHPs} on {LV} distribution networks. Real electricity and heat profiles are taken as a starting point of the studies. Both Air Source Heat Pump ({ASHP}) and Ground Source Heat Pump ({GSHP}) types are modeled as black boxes with performance and heat capacity characteristics changing with operating conditions according to manufacturers’ curves, addressing in particular the need for and impact of different types of Auxiliary Heating ({AH}) systems. A specific {LV} network analysis tool has been built that integrates the three-phase unbalanced power flow solution engine {OpenDSS} with the developed {EHP} models and is capable of properly addressing single-phase connections, adequately modeling the unbalanced nature of {LV} networks. Different metrics are used to quantify the impact of the considered technologies, with emphasis on thermal and voltage limits, according to current engineering standards. To cope with the many relevant uncertainties ({EHP} size, location in the network, operation pattern, reactive power consumption, network headroom, etc.), various case studies and sensitivity analyses have been carried out for representative suburban areas in the {UK} and for different scenarios in order to exemplify the developed methodology and illustrate the main drivers for impact and trends in the different cases. The tool can be adapted to perform studies for different situations and scenarios and can be used as decision making support by network operators, energy planners, policy makers, and so on, to better quantify the potential implications of large scale electrification of heating.},
	pages = {249--266},
	journaltitle = {Applied Energy},
	author = {Navarro-Espinosa, Alejandro and Mancarella, Pierluigi},
	date = {2014},
	keywords = {Air source heat pump, Auxiliary heater, Ground source heat pump, Low voltage distribution networks, {OpenDSS}, Unbalanced power flow},
}
@incollection{DINCER2015131,
title = {Chapter 4 - Heat Pump Systems},
editor = {Ibrahim Dincer and Marc A. Rosen},
booktitle = {Exergy Analysis of Heating, Refrigerating and Air Conditioning},
publisher = {Elsevier},
address = {Boston},
pages = {131-168},
year = {2015},
isbn = {978-0-12-417203-6},
doi = {https://doi.org/10.1016/B978-0-12-417203-6.00004-1},
url = {https://www.sciencedirect.com/science/article/pii/B9780124172036000041},
author = {Ibrahim Dincer and Marc A. Rosen},
keywords = {Exergy, Energy, Efficiency, Heating, Cooling, HVACR, Heat pumps},
abstract = {This chapter introduces and discusses heat pump systems along with their various performance criteria in detail for various heating, ventilation, air conditioning, and refrigeration applications. Heat pump systems are classified based on heat source and heat source/sink configuration. For illustrative purposes, a basic heat pump system is analyzed both energetically and exergetically. Some common residential and industrial applications of heat pumps are discussed, and four heat pump systems are comparatively assessed through numerous case studies, including (i) air source heat pump with circulating air, (ii) air source heat pump with ventilation, (iii) submerged water source heat pump (direct expansion), and (iv) open-loop water source heat pump. Energetic and exergetic coefficient of performances of these systems are determined and compared for specific conditions.}
}
@incollection{SARBU2017347,
title = {Chapter 9 - Solar-Assisted Heat Pumps},
editor = {Ioan Sarbu and Calin Sebarchievici},
booktitle = {Solar Heating and Cooling Systems},
publisher = {Academic Press},
pages = {347-410},
year = {2017},
isbn = {978-0-12-811662-3},
doi = {https://doi.org/10.1016/B978-0-12-811662-3.00009-8},
url = {https://www.sciencedirect.com/science/article/pii/B9780128116623000098},
author = {Ioan Sarbu and Calin Sebarchievici},
keywords = {Absorption heat pump, Air-source heat pump, Economic indicators, Energy efficiency, Experimental investigation, Greenhouse gas emissions, Ground-source heat pump, Heat pump selection, Heat pump types, Operation regimes, Simulation models, Thermodynamic cycle, Vapor-compression heat pump},
abstract = {This chapter presents the operation principle of a heat pump (HP), discusses the vapor-compression-based HP systems, and describes the thermodynamic cycle and the calculation, as well as operation regimes of a vapor-compression HP with electro-compressor. The calculation of greenhouse gas (GHG) emissions of HPs and energy and economic performance criteria that allow for implementing an HP in a heating/cooling system is considered. A detailed description of the HP types and ground-source HP (GSHP) development is presented and important information on the selection of the heat source and HP systems are discussed. Additionally, other approach is to integrate the solar thermal system on the source side of the HP so that the solar thermal energy is either the sole heat source for the HP or provides supplementary heat. Additionally, the operation principle and calculation of the thermodynamic cycle for a solar-assisted absorption HP are also briefly analyzed. Finally, analytical and experimental studies are performed on a direct-expansion solar-assisted heat pump (DX-SAHP) water heating system. The effects of various parameters, including solar radiation, ambient air temperature, collector area, storage volume and speed of compressor, are been investigated on the thermal performance of the DX-SAHP system. A novel heating, ventilating and air-conditioning system consisting of a solar-assisted absorption ground-coupled HP is also described, and some of the influence parameters on its energy efficiency is analyzed. A model of the experimental installation is developed using the TRNSYS software and validated with experimental results obtained in the installation for its cooling-mode operation.}
}
